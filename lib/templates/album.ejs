<blockquote>
    <p><%- quote; %></p>
</blockquote>

<style>
    <% include index.css %>
</style>

<div class="hexo-douban-tabs">
    <a class="hexo-douban-tab" id="hexo-douban-tab1" href="javascript:;"
       rel="external">
        <%= __('albumListening') %>
        (<%= listening.length %>)</a>
    <a class="hexo-douban-tab" id="hexo-douban-tab2" href="javascript:;"
       rel="external">
        <%= __('albumWish') %>
        (<%= wish.length %>)</a>
    <a class="hexo-douban-tab" id="hexo-douban-tab3" href="javascript:;"
       rel="external">
        <%= __('albumListened') %>
        (<%= listened.length %>)</a>
</div>
<div>
    <div id="hexo-douban-item1">
        <% listening.forEach(function(item){ %>
        <% include albumListening.ejs %>
        <% }); %>
        <% include pagination.ejs %>
    </div>
    <div id="hexo-douban-item2">
        <% wish.forEach(function(item){ %>
        <% include albumWish.ejs %>
        <% }); %>
        <% include pagination.ejs %>
    </div>
    <div id="hexo-douban-item3">
        <% listened.forEach(function(item){ %>
        <% include albumListened.ejs %>
        <% }); %>
        <% include pagination.ejs %>
    </div>
</div>

<script>
    <% include index.js %>
    <% include pagination.js %>
</script>

